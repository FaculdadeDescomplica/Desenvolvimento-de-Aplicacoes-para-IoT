// Código para controlar um LED com Arduino

// Define o pino do LED
const int ledPin = 13;

void setup() {
  // Configura o pino do LED como saída
  pinMode(ledPin, OUTPUT);
}

void loop() {
  // Liga o LED
  digitalWrite(ledPin, HIGH);
  // Aguarda 1 segundo
  delay(1000);
  // Desliga o LED
  digitalWrite(ledPin, LOW);
  // Aguarda 1 segundo
  delay(1000);
}
